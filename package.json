{
  "name": "react-styled-guide",
  "version": "0.2.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "Luiz Claudio",
  "homepage": "https://github.com/luizclr/react-styled-guide",
  "bugs": "https://github.com/luizclr/react-styled-guide/issues",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "test:coverage:watch": "yarn test:coverage --watch",
    "lint": "tsdx lint .",
    "lint:fix": "yarn lint --fix",
    "prepare": "tsdx build ",
    "prepare-husky": "husky install",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "styled-components": ">=5.3.8",
    "typescript": ">=4.9.5"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "printWidth": 100,
    "proseWrap": "always",
    "quoteProps": "as-needed",
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false,
    "endOfLine": "auto"
  },
  "module": "dist/react-styled-guide.esm.js",
  "size-limit": [
    {
      "path": "dist/react-styled-guide.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-styled-guide.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "babel-plugin-module-resolver": "^5.0.0",
    "husky": "^8.0.3",
    "jest-styled-components": "^7.1.1",
    "lint-staged": "^13.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.2.4",
    "styled-components": "^5.3.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  }
}
